local data = require(game.ReplicatedStorage.ClientModules.Core.ClientData).get_data()[game.Players.LocalPlayer.Name]
print("=== YOUR PETS LIST ===")
for i, v in pairs(data.inventory.pets) do
    print(string.format("[%d] Name: %s | ID: %s | Kind: %s", i, v.name or "?", v.id or "?", v.kind or "?"))
end
print("=== END ===")
